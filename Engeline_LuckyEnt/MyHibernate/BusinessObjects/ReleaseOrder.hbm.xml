<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="MyHibernate.BusinessObjects" assembly="MyHibernate">
  <!-- Mappings for class 'QuotationDetails' -->
  <class name="MyHibernate.BusinessObjects.ReleaseOrder, MyHibernate" table="ReleaseOrder" lazy="false">
    <!-- Identity mapping -->
    <id name="ID">
      <column name="ID" />
      <generator class="native" />
    </id>

    <property name="DR_Number" column="DR_Number" />
    <property name="Trx_Date" column="Trx_Date" />
    <property name="Driver" column="Driver" />
    <property name="Truck" column="Truck" />
    <property name="Notes" column="Notes" />
    <property name="Warehouse" column="Warehouse" />
  </class>
</hibernate-mapping>