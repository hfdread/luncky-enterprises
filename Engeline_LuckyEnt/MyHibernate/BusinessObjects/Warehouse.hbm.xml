<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="MyHibernate.BusinessObjects" assembly="MyHibernate">

  <!-- Mappings for class 'Warehouse' -->
  <class name="MyHibernate.BusinessObjects.Warehouse, MyHibernate" table="Warehouse" lazy="false">

    <!-- Identity mapping -->
    <id name="ID">
      <column name="ID" />
        <generator class="assigned" />
    </id>
   
    <property name="Name" />
    <property name="Address" />
    <property name="Phone" />
    <property name="Fax" />
    <property name="isDefault" />
    
  </class>
</hibernate-mapping>
