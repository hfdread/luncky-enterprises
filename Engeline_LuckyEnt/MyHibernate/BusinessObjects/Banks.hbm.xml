  <class name="MyHibernate.BusinessObjects.Banks, MyHibernate" table="Banks" lazy="false">
    <!-- Identity mapping -->
    <id name="ID">
      <column name="ID" />
      <generator class="native" />
    </id>

    <property name="Name" />
    <property name="ShortDescription" />
  </class>
</hibernate-mapping>